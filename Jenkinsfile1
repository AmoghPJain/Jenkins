pipeline{
    agent any

    stages {
        stage('checkout') {
            steps {
                checkout changelog: false,
         scm: scmGit(branches: [[name: 'main']]
                     userRemoteConfigs: [
                         [ credentialsId: 'slave_ssh',
                           url: 'https://github.com/AmoghPJain/Jenkins.git' ]
                         ])
            }
        }
        }
    }