pipeline{
    agent any

    stages {
        stage('checkout') {
            steps {
                checkout changelog: false,
         scm: scmGit(userRemoteConfigs: [
                         [ credentialsId: 'slave_ssh'
                           url: 'git@github.com:jenkinsci/git-client-plugin.git' ]
                         ])
            }
        }
        }
    }